{
    "name": "abiturma/php-fints",
    "description": "An adapter to use home banking with php",
    "type": "library",
    "keywords": [
        "banking",
        "hbci",
        "fints"
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Abiturma\\PhpFints\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Abiturma\\PhpFints\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Aaron Kunert",
            "email": "aaron.kunert@koona-labs.de"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=8.0",
        "ext-curl": "*",
        "ext-mbstring": "*",
        "ext-iconv": "*",
        "php-curl-class/php-curl-class": "^9.5",
        "genkgo/camt": "^2.0.0",
        "psr/log": "^2.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "config": {
        "sort-packages": true
    }
}
